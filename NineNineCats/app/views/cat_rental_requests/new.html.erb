<form action="<%=cat_rental_requests_url%>" method="post">
  <label for="cat_rental_requests[cat_id]">Requested Cat</label>
  <select name="cat_rental_requests[cat_id]">
    <% @catrentals.each  do |requested_cat|%>
      <%="<option value=\"#{requested_cat.id}\">#{requested_cat.name}</option>".html_safe %>
    <% end %>
  </select>
  <br>
  <label for="cat_rental_requests[start_date]">Start Date</label>
  <input type="date" name = "cat_rental_requests[start_date]">
  <br>
  <label for="cat_rental_requests[end_date]">End Date</label>
  <input type="date" name = "cat_rental_requests[end_date]">
  <br>
  <input type="submit" value="Rent Cat">
